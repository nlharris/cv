keywords = {GULO,OWL,logical definitions,ontology,phenotype,phenotype owl gulo reasoning ontology,reasoning},
mendeley-tags = {GULO,OWL,logical definitions,ontology,phenotype,reasoning},
number = {1},
pages = {418},
title = {{Improving ontologies by automatic reasoning and evaluation of logical definitions}},
