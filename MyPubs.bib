journal = {Genome Res},
keywords = { Genetic Human Internet Online Systems Software So, Non-U.S. Gov't Support, P.H.S. Systems Integration, U.S. Gov't,Algorithms Animals Biological Sciences/*methods/tr},
number = {10},
pages = {1611--1618},
title = {{The Bioperl toolkit: Perl modules for the life sciences}},
volume = {12},
year = {2002}
}
